    "babel-jest": "26.0.1",
    "jest": "26.0.1",
    "jest-transform-stub": "2.0.0",
    "node-sass": "4.14.1",
    "parcel-bundler": "1.12.4",
    "prettier": "2.0.5",
    "react-hot-loader": "4.12.21",
    "sass": "1.26.8"
