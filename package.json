{
  "name": "react-parcel-app",
  "version": "0.0.1",
  "description": "Simple React + Parcel set up.",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html --open",
    "build": "parcel build src/index.html",
    "test": "jest",
    "prettier": "prettier --check \"**/*.*\"",
    "prettier:write": "prettier --write \"**/*.*\"",
    "lint": "eslint 'src/**/*.{js,jsx}'",
